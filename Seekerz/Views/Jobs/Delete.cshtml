@model Seekerz.Models.Job

@{
    ViewData["Title"] = "Delete";
}



<h3 class="Mono">Are you sure you want to delete this?</h3>
<div>
    
    <hr />
    <dl class="dl-horizontal">
        <dt class="Mono">
            Position
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Position)
        </dd>
        <dt class="Mono">
           Personal Notes
        </dt>
        <dd>
            @Html.DisplayFor(model => model.PersonalNotes)
        </dd>
        <dt class="Mono">
            Status with NSS?
        </dt>
        <dd>
            @Html.DisplayFor(model => model.ToldNss)
        </dd>
        
       
        <dt class="Mono">
           Company Name
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Company.Location)
        </dd>
    </dl>
    <br />
    <br />

    <form asp-action="Delete">
        <input type="hidden" asp-for="JobId" />
        <input type="submit" value="Delete" class="btn btn-info DeleteButton" /> |
        <a asp-action="Index" class="btn btn-info BackButton">Back to List</a>
    </form>
</div>
